{
  "name": "@eagle/jetpack",
  "version": "1.0.0",
  "description": "Nifty convenience wrappers that speed up your daily work",
  "repository": {
    "type": "git",
    "url": "https://github.com/Graf009/jetpack.git"
  },
  "author": "Oleg Orlov <mail@orlovoleg.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "preversion": "npm run clean && npm run check && npm run build",
    "clean": "rimraf dist es",
    "lint": "eslint src",
    "check": "npm run lint",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es"
  },
  "dependencies": {
    "babel-runtime": "6.9.2",
    "react-link-state": "0.1.2"
  },
  "devDependencies": {
    "@eagle/eslint-config-eagle": "1.2.1",
    "babel-cli": "6.9.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-es2015-loose-native-modules": "1.0.0",
    "babel-preset-stage-0": "6.5.0",
    "cross-env": "1.0.8",
    "eslint": "2.11.0",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-jsx-a11y": "1.2.2",
    "eslint-plugin-react": "5.1.1",
    "rimraf": "2.5.2"
  }
}
